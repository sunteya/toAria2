{
    "name": "添加到aria2",
    "version": "1.1.0",
    "manifest_version": 3,
    "description": "把下载添加到aria2",
    "homepage_url": "https://github.com/sunteya/toAria2",
    "minimum_chrome_version": "88.0",
    "icons": {
        "16": "icon.png",
        "19": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "options_page": "options.html",
    "permissions": [
        "downloads",
        "notifications",
        "contextMenus",
        "storage"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "action": {
        "default_title": "暂停/启用"
    },
    "background": {
        "service_worker": "add.js",
        "type": "module"
    }
}
